{
    "version": 2,
    "functions": {
        "api/index.js": {
            "runtime": "nodejs18.x"
        }
    },
    "routes": [{
            "src": "/socket.io/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/board/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}